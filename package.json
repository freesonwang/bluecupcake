{
  "name": "es6-babel6",
  "version": "1.0.0",
  "description": "How to get started writing ES6 on the front end with babel 6+",
  "main": "index.js",
  "scripts": {
    "start": "npm run start-server & npm run gulp",
    "start-server": "python -m SimpleHTTPServer 3000",
    "gulp": "gulp"
  },
  "keywords": [
    "es6",
    "babel"
  ],
  "author": "Jean-Pierre Sierens",
  "license": "ISC",
  "devDependencies": {
    "aliasify": "^2.1.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.4.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "gulp": "3.9.1",
    "gulp-util": "3.0.7",
    "vinyl-source-stream": "1.1.0",
    "vue": "^2.3.2",
    "vueify": "^9.4.1"
  },
  "browserify": {
    "transform": [
      "aliasify",
      "vueify"
    ]
  },
  "aliasify": {
    "aliases": {
      "vue": "vue/dist/vue"
    }
  }
}
